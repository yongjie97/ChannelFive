'\n\n    export default {\n        name: 'Profile',\n        setup () {\n            return { v$: useVuelidate() }\n        },\n        data() {\n            return {\n                email: '',\n                displayName: '',\n                zipCode: '',\n                successMessage: null,\n                failMessage: null,\n            }\n        },\n        validations: {\n            email: {\n              required,\n              email\n            },\n            displayName: {\n              required\n            },\n            zipCode: {\n              required,\n              numeric,\n              minLength: minLength(6),\n              maxLength: maxLength(6)\n            }\n        },\n        mounted() {\n            axios({method:'get', \n                    url: \"http://localhost:8080/profile/retrieve\", \n                    withCredentials: true\n            })\n            .then(response => {\n                if (response != null) {\n                    this.email = response.data.data.email\n                    this.displayName = response.data.data.displayName\n                    this.zipCode = response.data.data.zipCode\n                }\n            }).catch((error) => {\n                this.failMessage = \"Opps! Something went wrong. Please try again later.\"\n                this.successMessage = null\n            })\n        },\n    }\n</script>"]},"metadata":{},"sourceType":"module"}