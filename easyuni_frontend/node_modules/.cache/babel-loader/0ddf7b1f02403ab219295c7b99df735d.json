class=\"d-flex\">\n                    <div class=\"card-body p-4 p-lg-5 text-black\">\n                    <form id=\"profileForm\" action=\"http://localhost:8080/profile/update\" method=\"post\">    \n                        <h4 class=\"fw-normal mb-4 \"><strong>My Profile</strong></h4>\n                        <p style=\"white-space: pre-line\" class=\"fw-normal mb-3 alert alert-success\" v-if=\"successMessage\">{{ successMessage }}</p>\n                        <p style=\"white-space: pre-line\" class=\"fw-normal mb-3 alert alert-danger\" v-if=\"failMessage\">{{ failMessage }}</p>   \n                        <div class=\"form-outline mb-4\">\n                            <input type=\"email\" id=\"email\" name=\"email\" v-model=\"email\" class=\"form-control form-control-lg\" disabled />\n                            <label class=\"form-label\">Email</label>\n                        </div>          \n                        <div class=\"form-outline mb-4\">\n                        <input type=\"text\" id=\"displayName\" name=\"displayName\" v-model=\"displayName\" class=\"form-control form-control-lg\"  />\n                        <label class=\"form-label\" for=\"displayName\">Display Name</label>\n                        </div>  \n                        <div class=\"form-outline mb-4\">\n                            <input type=\"text\" id=\"zipCode\" name=\"zipCode\" v-model=\"zipCode\" class=\"form-control form-control-lg\"/>\n                            <label class=\"form-label\" for=\"zipCode\">Zip Code</label>\n                        </div>\n                        <div class=\"form-outline mb-4\">\n                            <a class=\"text-dark\" href=\"/profile/password\">Change Password</a>\n                        </div>\n                        <div class=\"pt-3\">\n                            <input class=\"btn btn-warning text-white btn-lg btn-block\" type=\"submit\" value=\"Save Profile\" />\n                        </div>\n                    </form>\n\n                    </div>\n                </div>\n                </div>\n            </div>\n            </div>\n        </div>\n        </div>\n    </section>\n</template>\n    \n<script>\n\n    import { useVuelidate } from '@vuelidate/core'\n    import { required, email, minLength, numberic } from '@vuelidate/validators'\n\n    export default {\n        name: 'Profile',\n        setup () {\n            return { v$: useVuelidate() }\n        },\n        data() {\n            return {\n                email: '',\n                displayName: '',\n                zipCode: '',\n            }\n        },\n        mounted() {\n            axios.get('http://localhost:8080/profile/retrieve', { withCredentials: true }) \n            .then(response => {\n                if (response != null) {\n                    this.email = response.data.data.email\n                    this.displayName = response.data.data.displayName\n                    this.zipCode = response.data.data.zipCode\n                }\n            })\n        },\n    }\n</script>"]},"metadata":{},"sourceType":"module"}